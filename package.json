{
   "name": "api-boilerplate",
   "version": "1.0.0",
   "description": "Simple API boilerplate with express and Ts",
   "main": "dist/index.js",
   "scripts": {
      "build": "node ./node_modules/typescript/bin/tsc",
      "watch": "tsc -w",
      "start": "ts-node src/index.ts",
      "dev": "node ./node_modules/nodemon/bin/nodemon.js dist/index.js",
      "builddev": "npm run build && npm run dev",
      "lint": "eslint . --ext .ts",
      "test": "jest '(\\.|/)(test)\\.tsx?$'",
      "test:unit": "jest '(\\.|/)(unit.test)\\.tsx?$'",
      "test:integration": "jest '(\\.|/)(int.test)\\.tsx?$'",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "typeorm:migration:generate": "(rm -rf dist || :) && npm run typeorm -- migration:generate -d src/config/ormconfig.ts ",
      "typeorm:migration:run": "(rm -rf dist || :) && npm run typeorm -- migration:run -d src/config/ormconfig.ts ",
      "typeorm:migration:revert": "(rm -rf dist || :) && npm run typeorm -- migration:revert  -d src/config/ormconfig.ts "
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Atrato-Pago/api-boilerplate.git"
   },
   "author": "Jose Toscano",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Atrato-Pago/api-boilerplate/issues"
   },
   "homepage": "https://github.com/Atrato-Pago/api-boilerplate#readme",
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/jest": "^29.0.0",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^5.36.1",
      "@typescript-eslint/parser": "^5.36.1",
      "eslint": "^8.23.0",
      "jest": "^28.0.0",
      "nodemon": "^2.0.19",
      "ts-jest": "^28.0.0",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.0.2",
      "express": "^4.18.1",
      "promise-mysql": "^5.2.0",
      "typeorm": "0.3.9",
      "reflect-metadata": "^0.1.13",
      "pg": "^8.4.0"
   }
}